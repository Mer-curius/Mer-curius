#include <iostream>
using namespace std;

int main() {
    int height, width;

    // Ask the user for height and width
    cout << "Enter height: ";
    cin >> height;
    cout << "Enter width: ";
    cin >> width;

    // Outer loop for each row
    for (int i = 0; i < height; i++) {
        // Inner loop for each column (asterisk)
        for (int j = 0; j < width; j++) {
            cout << "* ";
        }
        cout << endl;  // Move to the next line after one row is printed
    }

    return 0;
}
